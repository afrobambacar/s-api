{
  "root": true,
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["mocha", "lodash-fp", "prettier"],
  "rules": {
    "camelcase": 0,
    "function-paren-newline": 0,
    "func-names": 0,
    "global-require": 1,
    "indent": 0,
    "max-len": 0,
    "no-console": 0,
    "no-shadow": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-nested-ternary": 0,
    "import/no-dynamic-require": 1,
    "mocha/no-exclusive-tests": 1,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "lodash-fp/consistent-name": 0,
    "lodash-fp/no-extraneous-function-wrapping": 0,
    "lodash-fp/use-fp": 0,
    "prettier/prettier": 0
  },
  "extends": ["airbnb-base", "plugin:lodash-fp/recommended", "prettier"]
}
